<UserControl x:Class="DNDMapViewer.Token"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:DNDMapViewer"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid Margin="10" MouseRightButtonDown="elps_MouseRightButtonDown">
        <Ellipse Fill="White" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Name="elps" MouseRightButtonDown="elps_MouseRightButtonDown" Stroke="Black" StrokeThickness="1"/>
        <Viewbox>
            <TextBlock TextWrapping="Wrap" Text="T" Name="objectLabel"/>
        </Viewbox>
    </Grid>
</UserControl>
